<h1 class="logo landing-page-title">NetSearch</h1>

<form method="POST" action="/user" class="default-form">
  <h2>Signup</h2>
  <% if (error) { %>
    <span class="warning"><%= error %></span>
  <% } %>
  <input type="email" name="email" class="field">
  <input type="password" name="password" minlength="6" class="field">
  <input type="hidden" name="likes" value="">
  <input type="submit" value="submit" class="button">
</form>